{
  "name": "graphql-apollo-react-example",
  "version": "1.0.0",
  "description": "Express with GraphQL on server and React with Apollo client for client.",
  "main": "client/index.js",
  "scripts": {
    "build": "webpack -p",
    "dev": "npm-run-all --parallel json-server dev:server dev:client",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/server.js",
    "json-server": "json-server --watch db.json",
    "start:server": "node server/server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aofleejay/graphql-apollo-react-example.git"
  },
  "keywords": [
    "graphql",
    "apollo-client",
    "react-apollo",
    "graphql node",
    "graphql express"
  ],
  "author": "Kunapot Pairat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aofleejay/graphql-apollo-react-example/issues"
  },
  "homepage": "https://github.com/aofleejay/graphql-apollo-react-example#readme",
  "dependencies": {
    "apollo-cache-inmemory": "1.1.4",
    "apollo-client": "2.0.4",
    "apollo-link-http": "1.3.1",
    "apollo-server-express": "1.3.0",
    "apollo-test-utils": "0.3.2",
    "axios": "0.16.2",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.18.2",
    "cors": "2.8.3",
    "dataloader": "1.3.0",
    "enzyme": "2.9.1",
    "express": "4.16.2",
    "graphql": "0.11.7",
    "graphql-tag": "2.6.0",
    "graphql-tools": "2.13.0",
    "jest": "20.0.4",
    "json-server": "0.10.3",
    "loadash": "0.0.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "path": "0.12.7",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
