{
  "name": "graphql-apollo-react-example",
  "version": "1.0.0",
  "description": "Express with GraphQL on server and React with Apollo client for client.",
  "main": "client/index.js",
  "scripts": {
    "build": "webpack -p",
    "dev": "npm-run-all --parallel json-server dev:server dev:client",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/server.js",
    "json-server": "json-server --watch db.json",
    "start:server": "node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aofleejay/graphql-apollo-react-example.git"
  },
  "keywords": [
    "graphql",
    "apollo-client",
    "react-apollo",
    "graphql node",
    "graphql express"
  ],
  "author": "Kunapot Pairat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aofleejay/graphql-apollo-react-example/issues"
  },
  "homepage": "https://github.com/aofleejay/graphql-apollo-react-example#readme",
  "dependencies": {
    "apollo-client": "^1.7.0",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "graphql": "^0.10.3",
    "json-server": "^0.10.3",
    "loadash": "^0.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.0"
  }
}
